<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="@android:color/transparent" 
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="center"
    tools:context=".PushAndSex">
    
<TextView
    android:id="@+id/TextView01"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:text="@string/acercadeExplicacion" >
  
</TextView>

<ImageView
    android:id="@+id/idbiteware"
    android:layout_width="195dp"
    android:layout_height="299dp"
    android:layout_gravity="center"
    android:background="@android:color/transparent"
    android:contentDescription="@string/Acercade"
    android:src="@drawable/bitewarelogo2" />

<ImageButton
			       android:id="@+id/idBotonAtras"
			       android:layout_width="25dp"
			       android:layout_height="25dp"
			       android:layout_margin="10dp"
			       android:layout_gravity="center"
			       android:background="@drawable/cerraricono"
			       android:contentDescription="@string/Salir"
			       android:onClick="cerrar" />
</LinearLayout>
